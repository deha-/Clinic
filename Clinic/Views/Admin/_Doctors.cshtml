<div id="doctors"></div>

<div id="detailsDialogDoctor">
    <form id="detailsFormDoctor">
        <div class="details-form-field">
            <label for="FirstName">First name:</label>
            <input id="Doctor_FirstName" name="Doctor_FirstName" type="text" />
        </div>
        <div class="details-form-field">
            <label for="LastName">Last name:</label>
            <input id="Doctor_LastName" name="Doctor_LastName" type="text" />
        </div>
        <div class="details-form-field">
            <label for="PWZ">PWZ:</label>
            <input id="Doctor_PWZ" name="Doctor_PWZ" type="text" />
        </div>
        <div class="details-form-field">
            <label for="Login">Login:</label>
            <input id="Doctor_Login" name="Doctor_Login" type="text" />
        </div>
        <div class="details-form-field">
            <label for="Password">Password:</label>
            <input id="Doctor_Password" name="Doctor_Password" type="password" />
        </div>
        <div class="details-form-field">
            <label for="ConfirmPassword">Confirm password:</label>
            <input id="Doctor_ConfirmPassword" name="Doctor_ConfirmPassword" type="password" />
        </div>
        <div class="details-form-field">
            <button type="submit" id="Doctor_Save">Save</button>
        </div>
    </form>
</div>